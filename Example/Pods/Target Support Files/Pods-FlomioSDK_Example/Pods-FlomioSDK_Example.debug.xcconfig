ENABLE_BITCODE = NO
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = "${PODS_ROOT}/Headers/FlomioSDK" $(inherited) "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/FlomioSDK"
LIBRARY_SEARCH_PATHS = $(inherited) "$(PODS_ROOT)/FlomioSDK" "$PODS_CONFIGURATION_BUILD_DIR/FlomioSDK" "${PODS_ROOT}/../../FlomioSDK"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/FlomioSDK"
OTHER_LDFLAGS = $(inherited) -ObjC -l"FlomioSDK" -l"SDKClasses" -l"c++" -l"z"
PODS_BUILD_DIR = $BUILD_DIR
PODS_CONFIGURATION_BUILD_DIR = $PODS_BUILD_DIR/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)
PODS_PODFILE_DIR_PATH = ${SRCROOT}/.
PODS_ROOT = ${SRCROOT}/Pods
